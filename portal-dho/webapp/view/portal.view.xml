<mvc:View controllerName="portaldho.controller.portal"
        xmlns:mvc="sap.ui.core.mvc"
	    xmlns="sap.m"
        xmlns:tnt="sap.tnt"
        xmlns:core="sap.ui.core"
        xmlns:layout="sap.ui.layout"
	    height="100%">
    
    <Page title="{i18n>title}" showNavButton="true" >
       	<ObjectHeader
                    icon="test-resources/sap/m/images/Woman_04.png"
                    iconDensityAware="false"
                    iconAlt="Denise Smith"
                    imageShape="Circle"
                    responsive="true"
                    title="{usuarioLogado>/Nome}"
                    intro="Senior Developer"
                    class="sapUiResponsivePadding--header" >
             <!-- TODO atribuir dados consumo serviÃ§o -->
			<ObjectAttribute title="Email address" text="DeniseSmith@sap.com" active="true" />
			<ObjectAttribute title="Office Phone" text="+33 6 453 564" />
			<ObjectAttribute title="Functional Area" text="Development" />
	    </ObjectHeader>

        <!-- items="{/ZI_PROCESSOS}" -->
        <!-- <tnt:NavigationListItem visible="{ IsVisible }"  text="{ DescInfotipo }" icon="{ CaminhoIcone }" /> -->

        <FlexBox
				alignItems="Center"
				justifyContent="SpaceBetween" >
            
            <mvc:XMLView viewName="portaldho.view.NavBar" />
            <!-- <core:Fragment fragmentName="portaldho.fragment.DadosPessoais" type="XML" /> -->
        </FlexBox>
    </Page>
</mvc:View>
